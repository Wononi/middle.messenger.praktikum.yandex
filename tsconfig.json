{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2016",
    "declaration": false,
    "module": "es6",
    "strictNullChecks": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "lib": ["ES2017", "DOM"],
    "moduleResolution": "node",
    "allowJs": true
  }
}

